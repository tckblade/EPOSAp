<KBPROGRAMMING name="Keyboard Programming Dialog" left="500" top="50" width="470" height="235">
	<!-- Contains the layout for the keyboard programming editor
	     This consists of a floating dialog over the keyboard with the editing controls mounted on it -->
	
	<BACKGROUND image="kbprogramming_bg.bmp" TransparentColour="00FF00FF"/>

	<BUTTONS>
		<!-- don't need a type button any more  BUTTON id="BTN:TYPE" name="Button Type Button" left="10" top="10" width="101" height="41"  -->
		<!--  STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="0"/  -->
		<!--  STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="101" top="0"/  -->
		<!--  /BUTTON  -->
		<BUTTON id="BTN:EXIT" name="Exit Button" left="384" top="5" width="69" height="28">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="649" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="649" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:MERGE" name="Merge Button" left="5" top="172" width="75" height="60">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="0" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:SPLIT" name="Split Button" left="82" top="172" width="75" height="60">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="75" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="75" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:UPLEVEL" name="Up Level Button" left="309" top="39" width="74" height="41">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="718" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="718" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:DOWNLEVEL" name="Down Level Button" left="389" top="39" width="74" height="41">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="792" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="792" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:COPY" name="Copy Button" left="159" top="172" width="75" height="60">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="150" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="150" top="60" TransparentColour="00FF00FF"/>
