		<!--  STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="328"/  -->
		<!--  STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="101" top="328"/  -->
		<!--  /BUTTON  -->
		<BUTTON id="BTN:IMAGE" name="Image Button" left="317" top="127" width="99" height="41">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="550" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="550" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<!--  BUTTON id="BTN:UPSHAPE" name="Up Shape Button" left="230" top="500" width="101" height="41"  -->
		<!--  STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="287"/  -->
		<!--  STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="101" top="287"/  -->
		<!--  /BUTTON  -->
		<!--  BUTTON id="BTN:DOWNSHAPE" name="Down Shape Button" left="230" top="550" width="101" height="41"  -->
		<!--  STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="328"/  -->
		<!--  STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="101" top="328"/  -->
		<!--  /BUTTON  -->
		<BUTTON id="BTN:EDITTEXT" name="Edit Text Button" left="213" top="127" width="99" height="41">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="451" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="451" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:UNDO" name="Undo Button" left="313" top="172" width="75" height="60">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="300" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="300" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<BUTTON id="BTN:REDO" name="Redo Button" left="390" top="172" width="75" height="60">
			<STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="375" top="0" TransparentColour="00FF00FF"/>
			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="375" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
	</BUTTONS>
	
	<WINDOWS>
		<!-- you can have background images on these but I don't because it's easier not to -->
		<WINDOW id="WDW:COLOUR" name="Button background colour window" left="5" top="127" width="101" height="41">
			<TEXT left="5" top="5" width="91" height="31">
				<FONT face="Arial" size="30" align="centre" valign="centre" colour="00FFFFFF" highlightcolour="000000FF"/>
			</TEXT>
		</WINDOW>
		<WINDOW id="WDW:TEXTCOLOUR" name="Button text colour window" left="109" top="127" width="101" height="41">
			<TEXT left="5" top="5" width="91" height="31">
				<FONT face="Arial" size="30" align="centre" valign="centre" colour="00FFFFFF" highlightcolour="000000FF"/>
			</TEXT>
		</WINDOW>
		<WINDOW id="WDW:INFO" name="Button information window" left="5" top="83" width="254" height="41">
			<TEXT left="5" top="5" width="240" height="31">
				<FONT face="Arial" size="30" align="left" valign="centre" colour="00FFFFFF" highlightcolour="000000FF"/>
			</TEXT>
		</WINDOW>
		<WINDOW id="WDW:LEVEL" name="Level window" left="5" top="39" width="300" height="41">
			<TEXT left="5" top="5" width="290" height="31">
