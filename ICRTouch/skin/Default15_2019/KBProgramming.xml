			<STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="225" top="60" TransparentColour="00FF00FF"/>
		</BUTTON>
		<!-- don't really need up and down buttons  BUTTON id="BTN:UPFONT" name="Up Font Button" left="120" top="500" width="101" height="41"  -->			
		<!--  STATEIMAGE state="up" image="kbprogramming_buttons.bmp" left="0" top="287"/  -->
		<!--  STATEIMAGE state="down" image="kbprogramming_buttons.bmp" left="101" top="287"/  -->
		<!--  /BUTTON  -->
